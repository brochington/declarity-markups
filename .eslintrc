{
    "parser": "babel-eslint",
    "rules": {
        "indent": [
            0,
            2
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        # "semi": [
        #     2,
        #     "always"
        # ],
        # "no-undef": [1, ]
        "no-console": 0,
        "no-unused-var": 0,
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/react-in-jsx-scope": 0
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "modules": true,
        "classes": true
    },
    "plugins": [
        "react"
    ]
    # "globals": {
    #    "module": false,
    #    "require": false
    # }
}
